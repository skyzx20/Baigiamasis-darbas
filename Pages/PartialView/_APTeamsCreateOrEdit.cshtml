@model fantazijos_lyga.Duomenu_baze.Komanda
<form id="create-form" method="post" asp-page="APTeams" asp-route-id="@Model.ID" asp-page-handler="CreateOrEdit" onsubmit="return jQueryModalPost(this);">
    <div class="form-group row">
        <label class="col-md-3 col-form-label">Pavadinimas:</label>
        <div class="col-md-9">
            <input autocomplete="off" asp-for="Pavadinimas" class="form-control">
            <span asp-validation-for="Pavadinimas" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label class="col-md-3 col-form-label">Sutrumpintas pavadinimas</label>
        <div class="col-md-9">
            <input autocomplete="off" asp-for="ShortName" class="form-control">
            <span asp-validation-for="ShortName" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label class="col-md-3 col-form-label">Logo adresas</label>
        <div class="col-md-9">
            <input autocomplete="off" asp-for="Logo" class="form-control">
            <span asp-validation-for="Logo" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label class="col-md-3 col-form-label">Regionas:</label>
        <div class="col-md-9">
            <select asp-for="Region" class="form-control" asp-items="Model.regionoList"></select>
            <span asp-validation-for="Region" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group justify-content-between">
        <button type="button" class="btn btn-secondary close-button" data-dismiss="modal">Atšaukti</button>
        <button type="submit" class="btn btn-primary save-button">Išsaugoti</button>
    </div>
</form>
<script type="text/javascript" language=javascript>
    $.validator.unobtrusive.parse(document);
</script>